  "main": "lib/index.js",
  "files": [
    "/lib"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc --build tsconfig.build.json",
    "clean": "rimraf lib/*",
    "dev": "ts-node-dev --respawn --transpileOnly src",
    "docs": "typedoc",
    "generate-models": "ts-node utils/generate",
    "lint": "eslint --ext ts,js src/ test/",
    "lint:fix": "npm run lint -- --fix",
    "semantic-release": "npx @scaleleap/semantic-release-config",
    "start": "ts-node --transpile-only --pretty src",
    "test": "jest --testPathIgnorePatterns test/integration/ ",
    "test:integration": "POLLY_MODE=passthrough jest test/integration/ --setupFiles dotenv/config",
    "test:watch": "jest --watchAll",
    "postversion": "ts-node --transpile-only --pretty utils/bump-version.ts"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "aws4-axios": "2.4.3",
    "axios": "0.21.4",
    "http-status-codes": "2.1.4",
    "ts-error": "1.0.6"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "10.0.3",
    "@aws-sdk/client-sts": "3.38.0",
    "@openapitools/openapi-generator-cli": "2.4.13",
    "@scaleleap/amazon-marketplaces": "13.0.0",
    "@scaleleap/jest-polly": "1.5.51",
    "@scaleleap/utils": "1.9.58",
    "@types/fancy-log": "1.3.1",
    "@types/jest": "26.0.24",
    "@types/lodash": "4.14.176",
    "@types/node": "14.17.22",
    "danger": "10.7.0",
    "dotenv": "10.0.0",
    "env-var": "7.0.1",
    "fancy-log": "1.3.3",
    "global-agent": "3.0.0",
    "jest": "26.6.3",
    "lodash": "4.17.21",
    "openapi-types": "9.3.0",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.6",
    "ts-morph": "12.0.0",
    "ts-node": "10.4.0",
    "ts-node-dev": "1.1.8",
    "tsconfigs": "4.0.2",
    "typedoc": "0.22.7",
    "typescript": "4.4.4"
  },
  "keywords": [
    "amazon mws",
    "amazon selling partner",
    "amazon sp api",
    "nodejs",
    "selling partner api",
    "sp-api",
    "typescript"
